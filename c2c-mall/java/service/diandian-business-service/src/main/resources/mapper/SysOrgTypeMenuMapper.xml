<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.diandian.dubbo.business.mapper.SysOrgTypeMenuMapper">
    <select id="queryMenuIdListByOrgTypeId" parameterType="java.lang.Long" resultType="java.lang.Long">
        SELECT menu_id FROM sys_org_type_menu WHERE org_type_id = #{orgTypeId, jdbcType=BIGINT}
    </select>
</mapper>
